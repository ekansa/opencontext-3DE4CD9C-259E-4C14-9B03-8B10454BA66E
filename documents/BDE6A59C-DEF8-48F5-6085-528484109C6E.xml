    <oc:person_links>
      <oc:link href="http://opencontext.org/persons/1A016E4D-75C5-4AA5-127B-DD3338D946BD" cite="contributor">
        <oc:name>Chris Moon</oc:name>
        <oc:id>1A016E4D-75C5-4AA5-127B-DD3338D946BD</oc:id>
