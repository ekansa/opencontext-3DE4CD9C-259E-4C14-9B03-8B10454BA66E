      <oc:link href="http://opencontext.org/persons/0FB25CA9-3EEA-4DE5-D6E6-ABA671E38FC3" cite="contributor">
        <oc:name>Catherine P. Foster</oc:name>
        <oc:id>0FB25CA9-3EEA-4DE5-D6E6-ABA671E38FC3</oc:id>
